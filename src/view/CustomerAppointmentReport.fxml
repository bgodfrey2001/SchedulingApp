<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="282.0" prefWidth="334.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CustomerAppointmentReport">
   <children>
      <VBox layoutX="178.0" layoutY="14.0" prefHeight="200.0" prefWidth="141.0">
         <children>
            <ComboBox fx:id="monthPicker" prefWidth="150.0" />
            <ComboBox fx:id="typePicker" prefWidth="150.0" />
            <Label fx:id="numberOfAppointments" text="Label">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <Button mnemonicParsing="false" onAction="#generateReportClicked" text="Generate Report" />
         </children>
      </VBox>
      <VBox layoutX="14.0" layoutY="14.0" prefHeight="200.0" prefWidth="172.0">
         <children>
            <Label text="Month">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <Label layoutX="10.0" layoutY="10.0" text="Type of Appointment">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <Label layoutX="10.0" layoutY="35.0" text="Number">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
         </children>
      </VBox>
      <Button layoutX="268.0" layoutY="243.0" mnemonicParsing="false" onAction="#backButtonClicked" text="Back" />
   </children>
</AnchorPane>
